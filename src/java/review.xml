<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="review" namespace="/review" extends="struts-default">
        <action name="add" class="action.ReviewAction" method="add">
            <result name="success">/RedirectPage.jsp</result>
            <result name="input">/RedirectPage.jsp</result>
            <result name="error">/Error.html</result>
            <result name="notLoggedIn">/Login.jsp</result>
        </action>
        <action name="list" class="action.ReviewAction" method="list">
            <result name="success">/MyBooks.jsp</result>
            <result name="input">/RedirectPage.jsp</result>
            <result name="error">/Error.html</result>
            <result name="notLoggedIn">/Login.jsp</result>
        </action>
    </package> 
</struts>

